            document.addEventListener('DOMContentLoaded', function () {
                const button = document.getElementById('js-press-config-save-button');
                const nameJS = document.getElementById('name-js');
                const userJS = document.getElementById('username-js');
                const emailJS = document.getElementById('email-js');
                const passJS = document.getElementById('password-js');
                if (button) {
                    button.addEventListener('click', async () => {
                      createLoading();
                        console.log(`nama : ${nameJS.value}, username ${userJS.value}, email ${emailJS.value}, pass ${passJS.value}`)
                        try {
  const queryString = `name=${nameJS.value}&email=${emailJS.value}&username=${userJS.value}&password=${passJS.value}&confirmPassword=${passJS.value}`;
  const response = await fetch(`/api/setup?${queryString}`, {
    method: 'POST',
    headers: { 'Content-Type': 'application/json' }
  });

                            if (!response.ok) {
                              const data = await response.json();
                                throw new Error(data.msg);
                            }
                            const data = await response.json();
                            console.log(data.msg);
                            if(data.msg == 'Register Successful'){
                              window.location.href = '/login';
                            }
                            // Lakukan sesuatu dengan data yang diterima dari server
                        } catch (error) {
                          document.getElementById('error-text').textContent = error;
                          document.getElementById('container-18e5170b77c1').remove();
                            //   console.log(error)
                            // console.error('There was a problem with the fetch operation:', error);
                        }
                    });
                }
            });
