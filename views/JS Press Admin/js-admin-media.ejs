      <section id="js-canvas-container" class="js-canvas-container js-canvas js-hidden-canvas" lazydev="18df8e21de11">
        <img id="js-image-zoom" class="js-image-zoom js-zoom-this-image" lazydev="18df8e24e011" src="https://images.pexels.com/photos/8332573/pexels-photo-8332573.jpeg?auto=compress&amp;cs=tinysrgb&amp;w=1260&amp;h=750&amp;dpr=1">
        <i id="js-close-button-trigger" class="js-close-button-trigger js-close-button fa fa-close" lazydev="18df8e2bb7b1">
        
        </i>
        <style>
          .js-canvas-container {
                
                display: flex;
                width: 100dvw;
                height: 100svh;
                gap: 1em;
                flex-grow: 1;
                flex-wrap: wrap;
                justify-content: center;
                align-items: flex-start;
                align-content: center;
                flex-direction: row;
                overflow-x: hidden;
                overflow-y: hidden;
                background-color: rgba(0, 0, 0, 0.89);
                padding-top: 5px;
                padding-right: 5px;
                padding-bottom: 5px;
                padding-left: 5px;
                position: fixed;
                transition: 0.2s;
                z-index: 51;
                box-sizing: border-box;
                }
                
                
                                 .js-fade-in {
                   opacity: 0;
                   animation: fadeIn 0.7s ease-in-out 0.3s both;
                }
                
                @keyframes fadeIn {
                   from {
                       opacity: 0;
                   }
                   to {
                       opacity: 1;
                   }
                }
                
                .js-hidden-canvas{
                   display: none;
                }
                
                
                .js-image-zoom {
                
                display: block;
                width: 50svw;
                height: 80svh;
                transition: 0.6s;
                box-shadow:  0.1em 0.16em 0em 0em rgba(43, 43, 43, 0);
                object-fit: cover;
                }
                
                
                
                .js-image-zoom:hover {
                
                box-shadow:  0.04em 0.04em 1.36em -0.06em rgba(43, 43, 43, 0.86);
                }
                
                
                @media screen and (max-width: 900px) {
                .js-image-zoom {
                
                width: 90svw;
                height: 60svh;
                }
                
                }
                .js-close-button-trigger {
                
                background-color: rgba(0, 0, 0, 0);
                padding-top: 10px;
                padding-right: 10px;
                padding-bottom: 10px;
                padding-left: 10px;
                transition: 0.7s;
                cursor: pointer;
                color: #949494;
                font-size: 35px !important;
                }
                
                
                
                .js-close-button-trigger:hover {
                
                background-color: rgba(167, 37, 37, 0.5);
                }
        </style>
        
